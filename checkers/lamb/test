"\w.w ((\x.x) y) w -> \w.w y w" by E-AppFull {
    (w)[
        "w ((\x.x) y) w -> w y w" by E-App1 {
            "w ((\x.x) y) -> w y" by E-App2 {
                "(\x.x) y -> y" by E-AppAbs {}
            }
        }
    ]
}

